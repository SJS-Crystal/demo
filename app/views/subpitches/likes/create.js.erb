$('<%= "#new_like_#{@subpitch.id}" %>').remove();
$('<%= "#subpitch_#{@subpitch.id}" %>').find('.card').children('div').children('span').remove()
$('<%= "#subpitch_#{@subpitch.id}" %>').find('.card').children('div').append('<%= j render(partial: "subpitches/likes/unlike", locals: {subpitch: @subpitch, liked: liked(check_like(@subpitch))}) %>')
$('<%= "#subpitch_#{@subpitch.id}" %>').find('.card').children('div').prepend('<%= j render(partial: "subpitches/likes/count", locals: {subpitch: @subpitch}) %>')
